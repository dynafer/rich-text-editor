{
	"extends": "../../tsconfig.shared.json",
	"references": [
		{ "path": "../dynafer/utils" }
	],
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": "ts",
		"outDir": "build/lib",
		"tsBuildInfoFile": "build/tsconfig.tsbuildinfo",
		"paths": {
			"finer": ["finer/ts/Finer"],
			"finer/packages/*": ["ts/packages/*"],
			"finer/plugins/*": ["ts/plugins/*"]
		}
	},
	"include": [
		"ts/**/*.ts",
		"ts/packages/**/*.d.ts"
	]
}