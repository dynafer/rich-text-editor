{
  "name": "root",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/**/*"
  ],
  "devDependencies": {
    "@dynafer/build-toolkit": "^1.0.2",
    "@finer-group/eslint-plugin": "^0.0.5",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.4",
    "eslint": "^8.26.0",
    "patch-package": "^6.4.7",
    "rollup": "^3.2.3",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "preinstall": "node -e \"if(!process.env.npm_execpath.includes('yarn')) throw new Error('You must use yarn')\"",
    "postinstall": "patch-package",
    "build": "build-toolkit",
    "watch": "build-toolkit -w ./packages",
    "lint": "eslint packages --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finer-group/finer-editor.git"
  },
